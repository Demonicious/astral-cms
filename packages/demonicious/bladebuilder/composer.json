{
    "name": "demonicious/bladebuilder",
    "description": "A drag and drop pagebuilder to manage pages for Laravel.",
    "keywords": [
        "pagebuilder"
    ],
    "homepage": "https://github.com/Demonicious/BladeBuilder",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Mudassar Islam",
        "email": "demoncious@gmail.com"
    }],
    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "BladeBuilder\\": "src/",
            "Demonicious\\BladeBuilder\\": "src/"
        },
        "files": [
            "src/Core/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Demonicious\\BladeBuilder\\ServiceProvider"
            ],
            "aliases": {
                "PageBuilder": "Demonicious\\BladeBuilder\\Facade"
            }
        }
    }
}